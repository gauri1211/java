public class Maddition
    public static void main(String args[]){
        int A[][]={{(1,2,3),(4,5,6),(7,8,9)}};
        int B[][]={{(1,2,3),(6,5,4),(7,3,9)}};
        int [][]C=new int[3][3];
        for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
            C[i][j]=A[i][j]+B[i][j]
            System.out.println(C[i][j]+"");
            }        
            System.out.println();
        }
    }
